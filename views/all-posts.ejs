<%
    for (value in posts) {
        let post = posts[value] %>
        <div>
            <h1><%= post.title%></h1>
            <p>Posted on:<%=post.createdAt%></p>
            <p><img src="<%=post.img%>"></p>
            <p>By <%= post.author%></p>
            <p><%=post.description%></p>
            <form action="./chat-room/create">
                <input id="storyID" name="storyID"  value="<%= post._id %>" style="display: none;">
                <input id="storyImg" name="storyImg"  value="<%= post.img %>" style="display: none;">
                <input id="storyTitle" name="storyTitle"  value="<%= post.title %>" style="display: none;">
                <input id="storyAuthor" name="storyAuthor"  value="<%= post.author %>" style="display: none;">
                <input type="submit" value="Create room for this post">
            </form>
        </div><br>
<%  }
%>
